{
    "name": "@untitledui/file-icons",
    "version": "0.0.8",
    "description": "Official React library for Untitled UI File Icons",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "optimize:svg": "svgo -f icons/ -r",
        "build": "npm run build:icons && npm run generate:import-map && npm run build:tsc",
        "generate:import-map": "node scripts/generate-import-map.js",
        "build:tsc": "tsup && npm run replace-ext",
        "replace-ext": "find dist -type f \\( -name \"*.js\" -exec sed -i '' 's/FILE_EXT/js/g' {} + \\) -o \\( -name \"*.mjs\" -exec sed -i '' 's/FILE_EXT/mjs/g' {} + \\)",
        "build:icons": "svgr --no-index -- icons && mv ./src/icons/default/* ./src/icons && rm -rf ./src/icons/default",
        "release": "standard-version --release-as patch && git push --follow-tags && npm publish"
    },
    "files": [
        "README.md",
        "LICENSE",
        "dist"
    ],
    "keywords": [
        "untitledui",
        "file",
        "file-icons",
        "mime types",
        "mime type icons",
        "icons",
        "svg",
        "react"
    ],
    "author": "Dilshod Turobov",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@svgr/babel-plugin-remove-jsx-attribute": "^8.0.0",
        "@svgr/cli": "^8.1.0",
        "@types/react": "^19.1.9",
        "prettier": "^3.6.2",
        "react": "^19.1.1",
        "standard-version": "^9.5.0",
        "svgo": "^4.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "react": ">= 18"
    }
}
